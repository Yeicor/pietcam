{
  "name": "pietcam",
  "version": "0.0.1",
  "description": "A Piet interpreter for the web browser, using your webcam as input.",
  "source": "src/index.html",
  "scripts": {
    "dev": "parcel serve ./src/index.html --dist-dir ./dist --open",
    "build": "cross-env NODE_ENV=production parcel build src/index.html --no-source-maps --public-url .",
    "clean": "rimraf .cache/ && rimraf dist/ && rimraf build/"
  },
  "author": "Yeicor",
  "license": "MIT",
  "dependencies": {
    "vue": "^3.2.45",
    "vue-router": "^4.2.0",
    "vuetify": "^3.2.4",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@parcel/transformer-typescript-tsc": "^2.8.3",
    "@parcel/transformer-vue": "2.8.3",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.57.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^9.12.0",
    "parcel": "^2.8.2",
    "rimraf": "^3.0.2",
    "typescript": "*"
  },
  "browserslist": [
    "> 0.2%",
    "not dead"
  ],
  "publishConfig": {
    "access": "public"
  },
  "volta": {
    "node": "16.17.1",
    "yarn": "1.22.19"
  }
}
