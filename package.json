{
  "name": "pietcam",
  "version": "0.0.5",
  "description": "A Piet interpreter, capable of using your webcam as input.",
  "keywords": [
    "piet",
    "interpreter",
    "webcam"
  ],
  "author": "Yeicor",
  "license": "MIT",
  "targets": {
    "default": {
      "source": "src/index.ts",
      "engines": {
        "browsers": "> 0.5%, last 2 versions, not dead"
      }
    },
    "test": {
      "source": "src/index_test.ts",
      "engines": {
        "node": ">= 12"
      },
      "optimize": false
    }
  },
  "scripts": {
    "build": "parcel build --target default",
    "test": "parcel build --target test && node dist/test/index_test.js"
  },
  "dependencies": {
    "piet-interpreter": "^0.0.6",
    "pngjs": "^7.0.0"
  },
  "devDependencies": {
    "parcel": "^2.9.1"
  }
}
